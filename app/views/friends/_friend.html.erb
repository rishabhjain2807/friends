<div id="<%= dom_id friend %>" class="card" style="width: 18rem;">
  <div class="card-header">
     <%= friend.first_name %>  <%= friend.last_name %>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item"> <%= friend.email %></li>
    <li class="list-group-item">  <%= friend.phone %></li>
    <li class="list-group-item">  <%= friend.twitter %></li>
  </ul>
</div>